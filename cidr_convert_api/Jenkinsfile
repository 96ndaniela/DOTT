pipeline {
    agent any

    
    stages {
        stage('one: find the dockerfile') {
            steps {
                dir("cidr_convert_api/java/"){
                    sd 'pwd'
                    sh 'ls'
                    sd 'chmod +x build.sh'
                sh './build.sh'
            }
            }
        }
        stage('second step'){
            steps{
            sh 'echo "where"'
            }   
        }
        stage('third')
        {
            steps{
            sh 'echo "bruh"'
            }
        }
    }
}
